{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020"],
    "declaration": false,
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["node", "@azure/functions"]
  },
  "include": ["**/*.ts", "src/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}


